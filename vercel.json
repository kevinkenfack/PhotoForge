{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/assets/(.*)", "dest": "/assets/$1" },
      { "src": "/dist/(.*)", "dest": "/dist/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  